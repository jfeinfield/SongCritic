{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
    "max-len": ["error", {"code": 80, "tabWidth": 2}],
    "indent": ["error", 2],
    "no-trailing-spaces": "error",
    "no-multiple-empty-lines": ["error", {"max": 1}],
    "object-curly-spacing": ["error", "never"]
  }
}
